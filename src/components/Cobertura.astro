---
const municipios = [
	'Areguá',
	'Capiatá',
	'Fernando de la Mora',
	'Guarambaré',
	'Itá',
	'Itauguá',
	'Julián Augusto Saldívar',
	'Lambaré',
	'Limpio',
	'Luque',
	'Mariano Roque Alonso',
	'Nueva Italia',
	'Ñemby',
	'San Antonio',
	'San Lorenzo',
	'Villa Elisa',
	'Villeta',
	'Ypacaraí',
];
---

<ul class="grid grid-cols-1 sm:grid-cols-2 gap-4 text-left text-gray-700">
	{
		municipios.map((municipio) => (
			<li class="flex items-center space-x-3 rtl:space-x-reverse">
				<svg
					class="flex-shrink-0 w-3.5 h-3.5 text-green-400"
					aria-hidden="true"
					xmlns="http://www.w3.org/2000/svg"
					fill="none"
					viewBox="0 0 16 12">
					<path
						stroke="currentColor"
						stroke-linecap="round"
						stroke-linejoin="round"
						stroke-width="2"
						d="M1 5.917 5.724 10.5 15 1.5"
					/>
				</svg>
				<span>{municipio}</span>
			</li>
		))
	}
</ul>
